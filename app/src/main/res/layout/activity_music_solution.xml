        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9. Cici"
        android:textColor="#6B8E23"
        android:layout_below="@+id/textView38"
        android:layout_alignLeft="@+id/textView38"
        android:layout_alignStart="@+id/textView38" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10. I'd Rather go Blind"
        android:textColor="#6B8E23"
        android:layout_below="@+id/textView39"
        android:layout_alignRight="@+id/textView37"
        android:layout_alignEnd="@+id/textView37" />
</RelativeLayout>
